{
  "name": "winemanager-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.0.4"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "husky": "^1.2.0",
    "lint-staged": "^7.3.0",
    "nock": "^10.0.2",
    "prettier": "^1.15.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
